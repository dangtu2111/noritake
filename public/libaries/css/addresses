    <!-- Back to Top Button -->
    <button onclick="topFunction()" id="myBtn" title="Lên đầu trang"></button>
    <style>
        #myBtn {
            width: 40px;
            height: 40px;
            display: none;
            position: fixed;
            bottom: 80px;
            right: 23px;
            z-index: 999;
            font-size: 13px;
            border: none;
            outline: none;
            background-color: #d29f13;
            color: white;
            cursor: pointer;
            padding: 9px;
            border-radius: 30px;
            opacity: 0.5;
            transition: bottom 0.3s;
        }

        #myBtn:before {
            content: "\2191";
            font-size: 16px;
        }

        #myBtn:hover {
            bottom: 87px;
        }
    </style>
    <script>
        function scrollFunction() {
            var mybutton = document.getElementById("myBtn");
            let pageHeight = window.innerHeight;
            const width = Math.max(document.clientWidth || 0, window.innerWidth || 0);
            if (mybutton && width > 767) {
                if (document.body.scrollTop > pageHeight || document.documentElement.scrollTop > pageHeight) {
                    mybutton.style.display = "block";
                } else {
                    mybutton.style.display = "none";
                }
            }
        }

        function topFunction() {
            document.body.scrollIntoView({
                behavior: 'smooth',
                block: 'start'
            });
        }
    </script>
    <!-- Additional Custom Script -->
    <script>
        $(document).ready(function() {
            $("h1").each(function(index) {
                var originalText = $(this).text();
                var formattedText = originalText.toLowerCase().replace(/ /g, '-');
                var id = formattedText + '-' + (index + 1);
                $(this).attr("id", id);
            });
        });

        $('h1').each(function() {
            $(this).css("scroll-margin-top", "40px");
        });

        history.scrollRestoration = "manual";

        setTimeout(function() {
            $('#haravan-notification').remove();
        }, 5000);
    </script>
    </body></html>
